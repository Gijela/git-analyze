{
  "name": "gitingest-ts",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:example": "tsx examples/scanner.ts",
    "test:dependency": "tsx examples/test/dependency-analysis.ts",
    "test:branch": "tsx examples/test/branch-analysis.ts",
    "web:dev": "tsx examples/testServer/index.ts",
    "web:build": "tsup examples/testServer/index.ts --format esm --dts --clean",
    "test:tree-sitter": "tsx src/utils/index.ts"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^13.1.0",
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.4",
    "@vue/compiler-sfc": "^3.5.13",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "glob": "^11.0.0",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-static": "^5.0.0",
    "simple-git": "^3.27.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/koa-static": "^4.0.4",
    "@types/koa__cors": "^5.0.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2"
  }
}
