{
  "name": "gitingest-ts",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:example": "tsx examples/test.ts",
    "analyze:complexity": "tsx examples/analyze-complexity.ts",
    "analyze:changes": "tsx examples/analyze-changes.ts",
    "analyze:project": "tsx examples/analyze-project.ts",
    "analyze:quality": "tsx examples/analyze-quality.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "glob": "^11.0.0",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
