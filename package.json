{
  "name": "gitingest-ts",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "glob": "^11.0.0",
    "simple-git": "^3.27.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "tsup": "^8.3.5"
  }
}
